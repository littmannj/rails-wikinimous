<h1>Articles</h1>

<%= link_to "Add new article", new_article_path %>

<% @articles.each do |article| %>
  <h2><%= article.title %></h2>
  <p><%= article.content %></p>
  <%= link_to "More details", article_path(article) %>
   <%= link_to "Delete article", article_path(article), method: :delete %>
<% end %>


